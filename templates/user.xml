<templateSet group="user">
  <template name="--" value="// --------------------------------------------------------------------------" description="Comment divider" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="$oSession" value="$oSession = Factory::service('Session', 'nailsapp/module-auth');" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oUserModel" value="$oUserModel = Factory::model('User', 'nailsapp/module-auth');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oNow" value="$oNow = Factory::factory('DateTime');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oEmailer" value="$oEmailer = Factory::service('Emailer', 'nailsapp/module-email');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oDb" value="$oDb = Factory::service('Database');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oAsset" value="$oAsset = Factory::service('Asset');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
      <option name="PHP Class Member" value="false" />
    </context>
  </template>
  <template name="$oInput" value="$oInput = Factory::service('Input');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oOutput" value="$oOutput = Factory::service('Output');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="$oUri" value="$oUri = Factory::service('Uri');" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="base." value="base.$FUNCTION$ = function() {&#10;    $END$&#10;};" toReformat="false" toShortenFQNames="true">
    <variable name="FUNCTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>